{
	"name": "fenix",
	"displayName": "Fenix",
	"description": "Fenix is a powerful and easy to use project generator for Visual Studio Code",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:fenix.helloWorld"
	],
	"main": "./build/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fenix.helloWorld",
				"title": "Hello World"
			}
		],
		"configuration": {
			"title": "Fenix",
			"scope": "application",
			"properties": {
				"fenix.repos": {
					"type": "array",
					"default": []
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/node": "^12.11.7",
		"@types/node-fetch": "^2.5.5",
		"@types/vscode": "^1.43.0",
		"glob": "^7.1.5",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"node-fetch": "^2.6.0"
	}
}
