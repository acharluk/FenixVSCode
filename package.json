{
	"name": "fenix",
	"publisher": "ACharLuk",
	"repository": {
		"url": "https://github.com/acharluk/Fenix"
	},
	"icon": "logo.png",
	"displayName": "Fenix",
	"description": "Fenix is a powerful and easy to use project generator for Visual Studio Code",
	"version": "1.0.3",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other",
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:fenix.open",
		"onView:fenixQuickCreate",
		"onView:fenixRepositories",
		"onView:fenixEnvironment"
	],
	"main": "./build/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fenix.open",
				"title": "Fenix: Open Fenix"
			},
			{
				"command": "fenix.repo.delete",
				"title": "Fenix: Delete repository"
			}
		],
		"configuration": {
			"title": "Fenix",
			"scope": "application",
			"properties": {
				"fenix.repos": {
					"type": "array",
					"default": []
				},
				"fenix.runCommands": {
					"enum": [
						true,
						false,
						"ask"
					],
					"default": "ask"
				},
				"fenix.env": {
					"type": "object",
					"default": {}
				}
			}
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "fenixView",
					"title": "Fenix",
					"icon": "no1.png"
				}
			],
			"panel": [
				{
					"id": "fenix-panel",
					"title": "asdasd",
					"icon": "no1.png"
				}
			]
		},
		"views": {
			"fenixView": [
				{
					"id": "fenixQuickCreate",
					"name": "Quick create"
				},
				{
					"id": "fenixRepositories",
					"name": "Repositories"
				},
				{
					"id": "fenixEnvironment",
					"name": "Environment"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "fenix.open",
					"when": "view == fenixQuickCreate"
				}
			],
			"view/item/context": [
				{
					"command": "fenix.repo.delete",
					"when": "viewItem == fenix-new"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/node": "^12.12.31",
		"@types/node-fetch": "^2.5.5",
		"@types/vscode": "^1.43.0",
		"glob": "^7.1.5",
		"tslint": "^6.1.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"lua-in-js": "^2.2.0",
		"node-fetch": "^2.6.0"
	}
}